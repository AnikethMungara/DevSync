# Server Settings
PORT=8787
HOST=0.0.0.0
FRONTEND_URL=http://localhost:3000

# Workspace Settings
WORKSPACE_DIR=workspace
WRITE_DEBOUNCE_MS=300

# Database Settings
DATABASE_PATH=database.db

# Execution Settings
EXECUTION_TIMEOUT=30
MAX_OUTPUT_SIZE=1048576

# Logging
LOG_LEVEL=INFO

# AI Agent Settings
AI_PROVIDER=openai
AI_MODEL=gpt-4o-mini
ALLOWED_MODELS=gpt-4o-mini,gpt-4o,claude-3-5-sonnet-20241022,gemini-1.5-pro

# API Keys (fill in your keys below)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# Agent Limits
AGENT_MAX_TOKENS=2000
AGENT_MAX_RUN_SECONDS=60
AGENT_MAX_OUTPUT_CHARS=200000
AGENT_RATE_RPS=2
AGENT_ALLOW_TOOLS=fs,exec,http,search

# Sandbox Settings
WORKSPACE_ROOT=./workspace
DISABLE_NETWORK=false
